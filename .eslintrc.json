{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/jsx-no-bind": "warn",
        "react/jsx-props-no-spreading": "warn",
        "no-use-before-define": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off",
    }
}
